{
    "name": "flashneo",
    "version": "24.10.20",
    "description": "A free WebRTC browser-based video call",
    "main": "server.js",
    "scripts": {
        "start": "node app/src/server.js",
        "start-dev": "nodemon app/src/server.js",
        "test": "echo \"Error: no test specified\" && exit 1",
        "lint": "npx prettier --write .",
        "docker-build": "docker build --tag realmad/flashneo:auromage .",
        "docker-rmi": "docker images |grep '<none>' |awk '{print $3}' |xargs docker rmi",
        "docker-push": "docker push realmad/flashneo:auromage",
        "docker-pull": "docker pull realmad/flashneo:auromage",
        "docker-rmc": "docker container rm flashneo --force",
        "docker-run": "docker run -d -p 3000:3000 --name flashneo realmad/flashneo:auromage",
        "docker-rune": "docker run -d -p 3000:3000 -v .env:/src/.env:ro --name flashneo realmad/flashneo:auromage",
        "docker-run-vm": "docker run -d -p 3000:3000 -v .env:/src/.env:ro -v ./app/:/src/app/:ro -v ./public/:/src/public/:ro --name flashneo realmad/flashneo:auromage",
        "docker-start": "docker start flashneo",
        "docker-stop": "docker stop flashneo"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/imago-flash-neo/flash-p2p.git"
    },
    "keywords": [
        "webrtc",
        "socket.io",
        "p2p",
        "nodejs",
        "video",
        "audio",
        "openai",
        "chatgpt",
        "client",
        "server",
        "streaming"
    ],
    "author": "Imago Techs",
    "license": "AGPL-3.0",
    "homepage": "https://imago.us",
    "dependencies": {
        "@sentry/integrations": "^7.114.0",
        "@sentry/node": "^8.13.0",
        "axios": "^1.7.2",
        "body-parser": "^1.20.2",
        "colors": "^1.4.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "crypto-js": "^4.2.0",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "express-openid-connect": "^2.17.1",
        "jsonwebtoken": "^9.0.2",
        "js-yaml": "^4.1.0",
        "ngrok": "^5.0.0-beta.2",
        "nodemailer": "^6.9.14",
        "openai": "^4.52.3",
        "qs": "^6.12.2",
        "socket.io": "^4.7.5",
        "swagger-ui-express": "^5.0.1",
        "uuid": "10.0.0",
        "xss": "^1.0.15"
    },
    "devDependencies": {
        "node-fetch": "^3.3.2",
        "nodemon": "^3.1.4",
        "prettier": "3.3.2"
    }
}